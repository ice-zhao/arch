<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xunwei.collectdata.devices">
    <class name="Host" table="t_sys_host">
        <id name="id" column="ID">
            <generator class="increment"/>
        </id>
        <property name="hostNo" column= "HostNo" type="string"></property>
        <property name="RemoteServerAddr" column= "RemoteServerAddr" type="string"></property>
        <property name="buildingID" column= "BuId" type="integer"></property>
        <property name="parkId" column="ParkId" type="integer"></property>
        <property name="floor" column= "Floor" type="integer"></property>
    </class>
</hibernate-mapping>