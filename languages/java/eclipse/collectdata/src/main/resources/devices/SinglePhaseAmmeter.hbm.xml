<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xunwei.collectdata.devices">
    <class name="SinglePhaseAmmeter" table="t_dev_ammeter_one">
    	<id name="id" column="ID">
    		<generator class="increment"/>
    	</id>
        <property name="Ua"  column = "Ua" type="float"></property>
        <property name="Ia"  column = "Ia" type="float"></property>
        <property name="pa"  column = "Pa" type="float"></property>
        <property name="qa"  column = "Qa" type="float"></property>
        <property name="pfa"  column = "Pfa" type="float"></property>
        <property name="frq"  column = "Frq" type="float"></property>
        <property name="epwr"  column = "Epwr" type="float"></property>
        <property name="erq"  column = "Erq" type="float"></property>
        <!--<property name="Ia"  column = "Ia" type="float"></property>-->
        <!--<property name="ParkId"  column = "ParkId" type="int"></property>-->
        <!--<property name="BuildingId"  column = "BuildingId" type="int"></property>-->
        <property name="devId"  column = "DevId" type="int"></property>
        <property name="timestamp"  column = "Time" type="java.util.Date"></property>
        <property name="hostNo"  column = "HostNo" type="string"></property>
        <property name="devNo"  column = "DevNo" type="string"></property>
    </class>
</hibernate-mapping>