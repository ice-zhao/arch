<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xunwei.collectdata.devices">
    <class name="com.xunwei.collectdata.Entity" table="t_entity">
        <id name="id"  column= "ID" type="integer"></id>
        <property name="devId"  column= "DevId" type="string"></property>
        <property name="timestamp"  column="Time" type="java.util.Date"></property>
        <property name="schemaId"  column="SchemaId" type="integer"></property>
    </class>
</hibernate-mapping>